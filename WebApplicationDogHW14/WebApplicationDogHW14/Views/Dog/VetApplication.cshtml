@model DogViewModel
@{
    <form method="post">
    <div>
        <label>
            <input asp-for="NameOfDog"/>
        </label>
    </div>
    <div>
        <label>
            <input asp-for="NameOfOwner"/>
        </label>
    </div>
    <div>
        <label>
            <input asp-for="AgeOfDog"/>
        </label>
    </div>
    <div>
        <label>
            <input asp-for="WeightOfDog"/>
        </label>
    </div>
    <button type="submit">Submit</button>
</form>
}
